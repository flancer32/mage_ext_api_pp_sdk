    {
      "type": "vcs",
      "url": "https://github.com/flancer32/mage_ext_api_pp_sdk"
    }
  ],
  "require": {
    "composer/composer": "1.0.0-alpha9",
    "magento-hackathon/magento-composer-installer": "*",
