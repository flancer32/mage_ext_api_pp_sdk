{
  "name": "flancer32/mage_ext_api_pp_sdk_test",
  "type": "project",
  "license": "MIT",
  "homepage": "https://github.com/flancer32/mage_ext_api_pp_sdk",
  "description": "Magento extension ot use PayPal PHP SDK (https://github.com/paypal/PayPal-PHP-SDK).",
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.firegento.com"
