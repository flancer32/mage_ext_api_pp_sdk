    "magento/core": "~1.9",
    "flancer32/mage_ext_api_pp_sdk": "dev-master"
  },
  "extra": {
    "magento-root-dir": "mage",
    "magento-deploystrategy": "symlink",
    "magento-force": "override",
    "auto-append-gitignore": false
  },
  "minimum-stability": "dev"
}
