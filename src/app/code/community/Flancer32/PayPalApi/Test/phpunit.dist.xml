		 convertWarningsToExceptions="true"
		 processIsolation="false"
		 stopOnFailure="false"
		 syntaxCheck="true"
		 bootstrap="./phpunit_bootstrap.php">
	<testsuite name="Flancer32 PayPal PHP SDK Unit Test">
		<directory suffix="_UnitTest.php">.</directory>
	</testsuite>
</phpunit>